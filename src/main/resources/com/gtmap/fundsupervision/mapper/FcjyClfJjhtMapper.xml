<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.gtmap.fundsupervision.mapper.FcjyClfJjhtMapper">

    <select id = "findJjhtByHbh" parameterType="java.lang.String" resultType="com.gtmap.fundsupervision.entity.FcjyClfJjhtEntity">
        SELECT
            jjhtid,
            jjhtbh,
            wtlx,
            wtly,
            wtsj,
            wtdd,
            hbh,
            fwsyqzh,
            qlr,
            zjlx,
            zjhm,
            wtrdz,
            lxdh,
            fdcjjjgbh,
            fdcjjrbh,
            zt,
            sfyx,
            bz
        FROM
            fcjy_clf_jjht
        where
            hbh = #{hbh}
    </select>

</mapper>